<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.Facebook</id>
    <version>2.1.444</version>
    <title>Facebook for Zebble (Xamarin)</title>
    <description>Facebook Login, register and other functionality for Zebble apps.</description>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.Facebook/master/Shared/Icon.png</iconUrl>
    <authors>Geeks Ltd</authors>
    <owners>Geeks Ltd</owners>
    <copyright>Copyright © 2019 Geeks Ltd - All rights reserved.</copyright>
    <projectUrl>https://github.com/Geeksltd/Zebble.Facebook</projectUrl>
    <dependencies>
      <group targetFramework="MonoAndroid">
        <dependency id="Xamarin.Facebook.Android" version="4.38.0" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Xamarin.Facebook.iOS" version="4.39.1" />
      </group>
      <group targetFramework="uap">
        <dependency id="Zebble.WebView" version="4.2.239" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.Facebook.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.Facebook.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.Facebook.UWP.*" target="lib\uap10.0" />
    <file src="..\..\lib\bin\ARM\Release\Zebble.Facebook.UWP.*" target="runtimes\win10-arm\lib\uap10.0" />
    <file src="..\..\lib\bin\x64\Release\Zebble.Facebook.UWP.*" target="runtimes\win10-x64\lib\uap10.0" />
    <file src="..\..\lib\bin\x86\Release\Zebble.Facebook.UWP.*" target="runtimes\win10-x86\lib\uap10.0" />
  </files>
</package>